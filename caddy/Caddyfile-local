https://localhost {
        tls internal
          
        handle /api/* {
            reverse_proxy gpt-researcher:8000
        }
        handle /getConfig {
            reverse_proxy gpt-researcher:8000
        }
        
        handle /ws* {
            reverse_proxy gpt-researcher:8000
            }

        handle * {
            reverse_proxy gptr-nextjs:3000
        }    
    
}

