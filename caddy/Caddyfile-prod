https://142a-178-150-77-146.ngrok-free.app  {
    tls internal

        handle /api/* {
            reverse_proxy gpt-researcher:8000
        }
        handle /getConfig {
            reverse_proxy gpt-researcher:8000
        }        
        handle /ws* {
            reverse_proxy gpt-researcher:8000
            }
        handle /outputs/* {
            reverse_proxy gpt-researcher:8000
            }

        handle * {
            reverse_proxy gptr-nextjs:3000

        }      

}

https://localhost {
    tls internal
    route {        
        handle /api/* {
            reverse_proxy gpt-researcher:8000
        }
        handle /getConfig {
            reverse_proxy gpt-researcher:8000
        }
        
        handle /ws* {
            reverse_proxy gpt-researcher:8000
            }

        handle * {
            reverse_proxy gptr-nextjs:3000

        }      
        
    }
}

